light_is_on = True

if light_is_on:
    print("The light is on!")
else:
    print("We're in the dark")
    
weight= 72.6

if weight <= 60:
    print("You're under weight")
if weight != 70:
    print("You're not the right weight")
if weight ==70:
    print("You're at your goal weight!")
if weight >70:
    print("you're ever so slightly overweight :(")
    
age = 26

if age >= 18:
    print("You are an adult!")
else:
    print("You are a child!")

